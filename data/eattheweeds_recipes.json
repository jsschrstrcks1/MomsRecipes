      "title": "Dandelion Wine",
      "category": "beverages",
      "attribution": "Green Deane, Eat The Weeds",
      "source_note": "https://www.eattheweeds.com/dandelions-hear-them-roar/",
      "description": "Traditional dandelion flower wine requiring 6-12 months aging",
      "servings_yield": "1 gallon",
      "prep_time": "",
      "cook_time": "",
      "total_time": "6-12 months (fermentation and aging)",
      "ingredients": [
        {"item": "dandelion flowers", "quantity": "3", "unit": "quart", "prep_note": "fresh, all green parts removed"},
        {"item": "golden raisins", "quantity": "1", "unit": "lb", "prep_note": ""},
        {"item": "water", "quantity": "1", "unit": "gallon", "prep_note": ""},
        {"item": "granulated sugar", "quantity": "3", "unit": "lb", "prep_note": ""},
        {"item": "lemons", "quantity": "2", "unit": "", "prep_note": ""},
        {"item": "orange", "quantity": "1", "unit": "", "prep_note": ""},
        {"item": "yeast and nutrient", "quantity": "", "unit": "", "prep_note": "wine yeast"}
      ],
      "instructions": [
        {"step": 1, "text": "Pick fresh flowers, trim stalks, remove all green parts if possible"},
        {"step": 2, "text": "Place flowers in large bowl; set aside 1 pint of water"},
        {"step": 3, "text": "Bring remaining gallon to boil; pour over flowers"},
        {"step": 4, "text": "Cover tightly with plastic wrap; leave 2 days, stirring twice daily"},
        {"step": 5, "text": "Transfer flowers and water to large pot; bring to low boil"},
        {"step": 6, "text": "Add sugar and thinly peeled citrus (avoiding white pith)"},
        {"step": 7, "text": "Maintain low boil for 1 hour; pour into fermenter"},
        {"step": 8, "text": "Add citrus juice and pulp; allow cooling"},
        {"step": 9, "text": "Add yeast and yeast nutrient; cover and place in warm location for 3 days"},
        {"step": 10, "text": "Strain into secondary fermenter"},
        {"step": 11, "text": "Add raisins and fit fermentation lock"},
        {"step": 12, "text": "Strain and rack after wine clears, topping with water"},
        {"step": 13, "text": "Leave until fermentation stops completely; rack again"},
        {"step": 14, "text": "Wait 2 months, then rack and bottle"},
        {"step": 15, "text": "Age 6 months to 1 year before consuming"}
      ],
      "temperature": "",
      "pan_size": "fermenter/carboy",
      "notes": ["Use only yellow petals, remove all green parts", "Traditional wine requiring patience for best results"],
