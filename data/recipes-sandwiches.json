    {
      "id": "egg-pita-sandwiches-bhg",
      "collection": "mommom",
      "collection_display": "MomMom Baker",
      "title": "Egg-Pita Sandwiches",
      "category": "sandwiches",
      "attribution": "Better Homes and Gardens New Cook Book",
      "source_note": "Cookbook page 149",
      "ingredients": [
        {
          "item": "sliced fresh mushrooms",
          "quantity": "1/3",
          "unit": "cup"
        },
        {
          "item": "sliced green onion or chopped green pepper",
          "quantity": "1/4",
          "unit": "cup"
        },
        {
          "item": "margarine or butter",
          "quantity": "1",
          "unit": "tbsp"
        },
        {
          "item": "eggs",
          "quantity": "3",
          "unit": ""
        },
        {
          "item": "milk",
          "quantity": "3",
          "unit": "tbsp"
        },
        {
          "item": "dried basil, crushed",
          "quantity": "1/8",
          "unit": "tsp"
        },
        {
          "item": "salt",
          "quantity": "",
          "unit": "dash"
        },
        {
          "item": "pepper",
          "quantity": "",
          "unit": "dash"
        },
        {
          "item": "shredded cheddar cheese",
          "quantity": "1/4",
          "unit": "cup"
        },
        {
          "item": "pita bread round",
          "quantity": "1",
          "unit": ""
        }
      ],
      "instructions": [
        {
          "step": 1,
          "text": "In a medium skillet cook mushrooms and green onion or chopped green pepper in margarine or butter till tender. In a bowl beat together 3 eggs; 3 tablespoons milk; 1/8 teaspoon dried basil, crushed; dash salt; and dash pepper. Pour over mushroom mixture. Cook, without stirring, till eggs begin to set on bottom and around edge. Lift and fold partially cooked eggs so uncooked portion flows underneath. Continue cooking till thoroughly cooked but still glossy and moist."
        },
        {
          "step": 2,
          "text": "Sprinkle 1/4 cup shredded cheddar cheese over eggs. Remove from heat. Let stand 1 minute or till cheese melts slightly. Halve 1 pita bread round; spoon egg mixture into halves."
        }
      ],
